        "df = pd.read_csv('https://raw.githubusercontent.com/Datamanim/pandas/main/BankChurnersUp.csv')\n",
        "df['Income_Category']\n",
        "\n",
        "def change(x):\n",
        "  if x =='Unknown':\n",
        "    return 'N'\n",
        "  elif x == 'Less than $40K':\n",
        "    return 'a'\n",
        "  elif x == '$40K - $60K' :\n",
        "    return 'b'\n",
        "  elif x == '$60K - $80K' :\n",
        "    return 'c'\n",
        "  elif x == '$80K - $120K' :\n",
        "    return 'd'\n",
        "  elif x =='$120K +' :\n",
        "    return 'e'\n",
        "\n",
        "dic = {'Unknown':'N','Less than $40K':'a','$40K - $60K':'b','$60K - $80K':'c',\n",
        "       '$80K - $120K':'d','$120K +':'e'}\n",
        "df['newIncome2'] = df['Income_Category'].map(lambda x: dic[x])\n",
        "\n",
        "df['newIncome'] = df['Income_Category'].apply(change)\n",
        "df['newIncome2']"
