        "# 결측치 처리\n",
        "data = train['OCCUPATION_TYPE'].mode()[0]\n",
        "train['OCCUPATION_TYPE'] = train['OCCUPATION_TYPE'].fillna(data)\n",
        "test['OCCUPATION_TYPE'] = test['OCCUPATION_TYPE'].fillna(data)"
      ],
      "metadata": {
        "id": "D8VkJoAs1JDa"
      },
      "execution_count": 107,
