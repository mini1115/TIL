        "from google.colab import files\n",
        "uploaded = files.upload()"
      ],
      "metadata": {
        "id": "ypj90TMMlkN0",
        "outputId": "91f4a106-f95c-4710-8b15-4a383a063070",
        "colab": {
          "base_uri": "https://localhost:8080/",
