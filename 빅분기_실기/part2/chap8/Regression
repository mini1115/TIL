      "source": [
        "from google.colab import files\n",
        "uploaded = files.upload()"
      ],
      "metadata": {
        "id": "kmgI8aIZv812",
        "outputId": "35064e4a-7d88-4281-c695-45ff451e4891",
        "colab": {
