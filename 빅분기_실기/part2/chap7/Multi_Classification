        "from google.colab import files\n",
        "uploaded = files.upload()"
      ],
      "metadata": {
        "id": "snvhpFSamal1",
        "outputId": "6680e1eb-18f6-4a66-c73b-a5b3c1972958",
        "colab": {
          "base_uri": "https://localhost:8080/",
