      "source": [
        "from google.colab import files\n",
        "uploaded = files.upload()"
      ],
      "metadata": {
        "id": "uAX996Lkj0x5",
        "outputId": "9392db65-cc9b-4483-dce9-a0a02c661d96",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 113
        }
