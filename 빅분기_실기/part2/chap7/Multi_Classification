      "cell_type": "code",
      "source": [
        "from google.colab import files\n",
        "uploaded = files.upload()"
      ],
      "metadata": {
        "id": "9qr8RuyHf1VF",
        "outputId": "c8ae70e3-24a2-4e68-ee07-4b981b1248cc",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 113
